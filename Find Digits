#include <stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
    int n,m;
       int i=0,j;
       scanf("%d",&n);
       m= n;
       while(m> 0)
       {
           j=m%10;
           if(j!= 0 && n%j==0) 
          i++;
          m=m/10;
       }
       printf("%d\n",i);
    }
    return 0;
}
  
