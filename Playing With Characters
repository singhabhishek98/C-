/*Playing With Characters*/

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
//#define MAX_LEN 15
int main()
{
    char ch;
    char s[12];    //[] is the scanset character.
    char sen[30];
    scanf("%c\n%s\n%[^\n]s",&ch,&s,&sen); 
    // ^\n stands for taking input until a newline isn't encountered.
    printf("%c\n%s\n%s",ch,s,sen);
    return 0;
}
