#include<stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
     int a[n]; 
    for (int i=0;i<n;i++)
    scanf("%d", &a[i]);
    
    int  max;
    max=a[0];
    for (int i=0;i<n;i++)
   {
    if(max<a[i])
    max=a[i];
    }
    
    int m=0;
    for(int i=0;i<n;i++)
   {
    if(a[i]==max)
    m++;
    }
        printf("%d",m);
        return 0;
  }
